<?xml version="1.0" encoding="UTF-8"?>
<arboles>
    <arbol id="acer_monspessulanum">
        <nombre_comun>Arce de Montpellier, Arce menor</nombre_comun>
        <vegetacion>Caducifolio</vegetacion>
        <altura>De 6 a 10 metros</altura>
        <forma_y_estructura>Copa   esférica.   Tronco   principal   recto   con bifurcaciones</forma_y_estructura>
        <ramaje_colgante></ramaje_colgante>
        <color_en_primavera>Haz verde brillante, envés verde blanquecino</color_en_primavera>
        <resistencia_a_las_heladas>Heladas fuertes (hasta -15ºC) </resistencia_a_las_heladas>
    </arbol>
<arbol id="olea_europea">
    <nombre_comun>Olivo</nombre_comun>
    <vegetacion>Perenne</vegetacion>
    <altura>De 8 a 15 metros</altura>
    <forma_y_estructura>Copa irregular. Tronco principal irregular con bifurcaciones.</forma_y_estructura>
    <ramaje_colgante>tortuoso</ramaje_colgante>
    <color_en_primavera>Haz verde oscuro, envés verde plateado</color_en_primavera>
    <resistencia_a_las_heladas>Heladas medias (hasta -10ºC)</resistencia_a_las_heladas>
</arbol>
<arbol id="platanus_orientalis">
    <nombre_comun>Platano</nombre_comun>
    <vegetacion>Caducifolio</vegetacion>
    <altura>De 20 a 25 metros</altura>
    <forma_y_estructura>Copa ovoidal. Tronco principal recto</forma_y_estructura>
    <ramaje_colgante>Expandido</ramaje_colgante>
    <color_en_primavera>Haz verde medio, enves verde claro. Color en otono: ocre</color_en_primavera>
    <resistencia_a_las_heladas>Heladas fuertes (hasta -20ºC))</resistencia_a_las_heladas>
</arbol>
<arbol id="quercus_ilex">
    <nombre_comun>Encina</nombre_comun>
    <vegetacion>Perenne</vegetacion>
    <altura>En torno a 25 metros</altura>
    <forma_y_estructura>Copa esférica o elíptica irregular. Tronco principal recto.</forma_y_estructura>
    <ramaje_colgante>tortuoso</ramaje_colgante>
    <color_en_primavera>Plateado en hojas jóvenes. En hojas antiguas, haz verde oscuro, envés plateado</color_en_primavera>
    <resistencia_a_las_heladas>Heladas fuertes (hasta -15ºC)</resistencia_a_las_heladas>
</arbol>

</arboles>